{
  "name": "ui-component-flag",
  "version": "1.0.0",
  "description": "Flags are used to highlight taxonomy terms or 'tags' on elements. For example 'special offer' or 'limited time only'",
  "main": "index.js",
  "scripts": {
    "build-dev": "./scripts/build-dev.sh",
    "build-dist": "./scripts/build-dist.sh",
    "test": "jest"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "dependencies": {
    "reactify": "~1.1.0",
    "redirectify": "~1.3.0",
    "react-tools": "~0.13.1",
    "react": "~0.13.1",
    "jest-cli": "~0.4.0",
    "browserify": "~9.0.8",
    "classnames": "~1.2.0"
  }
}
